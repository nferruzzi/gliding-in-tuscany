{
  "name": "gliding-tuscany",
  "version": "1.0.0",
  "description": "Aeroagriturismo e scuola di volo a vela in Toscana",
  "scripts": {
    "css:build": "tailwindcss -i ./src/css/style.css -o ./src/css/output.css --minify",
    "css:watch": "tailwindcss -i ./src/css/style.css -o ./src/css/output.css --watch",
    "eleventy:build": "eleventy",
    "eleventy:serve": "eleventy --serve",
    "build": "npm run css:build && npm run eleventy:build",
    "start": "npm run css:build && npm run eleventy:serve",
    "dev": "npm run start"
  },
  "keywords": ["agriturismo", "volo a vela", "toscana"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.3.6"
  }
}
